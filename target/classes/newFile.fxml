<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane prefHeight="594.0" prefWidth="866.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NewFile">
   <children>
      <Button fx:id="startButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#startAction" style="-fx-background-color: green;" text="start" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <TextFlow fx:id="soilDataField" layoutX="14.0" layoutY="111.0" prefHeight="151.0" prefWidth="165.0" style="-fx-background-color: grey; -fx-text-alignment: center;" />
      <Button fx:id="getSoilDataButton" layoutX="14.0" layoutY="61.0" mnemonicParsing="false" onAction="#getSoilDataAction" text="Get Soil Data" />
      <Button layoutX="88.0" layoutY="15.0" mnemonicParsing="false" style="-fx-background-color: red;" text="stop" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="streamSoilDataButton" layoutX="118.0" layoutY="61.0" mnemonicParsing="false" onAction="#streamSoilDataAction" text="Stream Soil Data" />
      <Line endX="-100.0" endY="-150.0" layoutX="374.0" layoutY="166.0" startX="-100.0" startY="415.0" />
      <Button fx:id="startIrrigationServerButton" layoutX="285.0" layoutY="16.0" mnemonicParsing="false" onAction="#startIrrigationServerAction" text="Start" />
      <Button fx:id="stopIrrigationServerButton" layoutX="351.0" layoutY="16.0" mnemonicParsing="false" onAction="#stopIrrigationServerAction" text="Stop" />
      <Line endX="-100.0" endY="-150.0" layoutX="670.0" layoutY="164.0" startX="-100.0" startY="415.0" />
      <TextFlow fx:id="irrigationData" layoutX="285.0" layoutY="121.0" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: grey;" />
      <Button fx:id="startIrrigationSensorButton" layoutX="280.0" layoutY="61.0" mnemonicParsing="false" onAction="#startIrrigationSensorAction" text="Button" />
   </children>
</AnchorPane>
